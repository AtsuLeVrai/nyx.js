{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "extends": ["../../biome.json"],
  "overrides": [
    {
      "include": ["src"],
      "linter": {
        "rules": {
          "suspicious": {
            "noReactSpecificProps": "off",
            "noArrayIndexKey": "off"
          },
          "nursery": {
            "useExplicitType": "off"
          },
          "style": {
            "noDefaultExport": "off"
          }
        }
      }
    }
  ]
}
