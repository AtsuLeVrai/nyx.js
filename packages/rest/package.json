{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nyxjs/rest",
  "version": "1.0.0",
  "description": "Type-Safe Discord REST API Client with Advanced Rate Limiting",
  "readme": "README.md",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AtsuLeVrai/nyx.js",
    "directory": "packages/rest"
  },
  "bugs": {
    "url": "https://github.com/AtsuLeVrai/nyx.js/issues"
  },
  "author": {
    "name": "AtsuLeVrai",
    "url": "https://github.com/AtsuLeVrai"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=development node ../../scripts/build.js",
    "build:prod": "cross-env NODE_ENV=production node ../../scripts/build.js",
    "check": "biome check --write --unsafe .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nyxjs/core": "workspace:^",
    "eventemitter3": "^5.0.1",
    "file-type": "^20.4.1",
    "form-data": "^4.0.2",
    "mime-types": "^3.0.1",
    "undici": "^7.6.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  },
  "optionalDependencies": {
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.14",
    "cross-env": "^7.0.3",
    "typescript": "^5.8.2",
    "undici-types": "^7.5.0"
  }
}
